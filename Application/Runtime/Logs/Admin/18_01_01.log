SQL: INSERT INTO `tp_blog` (`title`,`content`,`author`,`time`,`click`,`cid`) VALUES ('马克思主义学院思政课教学改革成果简介','<p style=\"text-indent:141px\"><br/></p><p><strong><span style=\"font-size:19px;font-family:宋体\">一、思政课情景剧教学法不断探索、成绩斐然。</span></strong></p><p style=\"text-indent:46px\"><span style=\"font-size:19px;font-family:宋体\">该项目</span><span style=\"font-size:19px\">2012</span><span style=\"font-size:19px;font-family: 宋体\">年获得辽宁省教学成果一等奖、</span><span style=\"font-size:19px\">2013</span><span style=\"font-size:19px;font-family:宋体\">年入选教育部首批思政课教学方法改革择优推广计划（全国</span><span style=\"font-size:19px\">20</span><span style=\"font-size:19px;font-family: 宋体\">项）。</span></p><p><strong><span style=\"font-size:19px;font-family:宋体\">二、“概论”课资源共享课建设与跨校选课扎实推进，成效显著。</span></strong></p><p><span style=\"font-size:19px\">&nbsp;&nbsp;&nbsp;&nbsp; </span><span style=\"font-size:19px;font-family:宋体\">“毛泽东思想和中国特色社会主义理论体系概论”课</span><span style=\"font-size:19px\">2013</span><span style=\"font-size:19px;font-family: 宋体\">年入选并建设成为辽宁省精品资源共享课。并在</span><span style=\"font-size:19px\">2014</span><span style=\"font-size:19px;font-family:宋体\">年成为辽宁省首批跨校选读学分课程。</span><span style=\"font-size: 19px\">2014-2017</span><span style=\"font-size:19px;font-family:宋体\">年连续被省内多所高校确定为跨校选读学分课程，获得了广泛的认可。</span></p><p><strong><span style=\"font-size:19px;font-family:宋体\">三、教学改革项目持续推进，不断探索</span></strong></p><p><span style=\"font-size:19px\">&nbsp;&nbsp; 2011-2017</span><span style=\"font-size:19px;font-family:宋体\">年，共获批辽宁省教改课题</span><span style=\"font-size:19px\">6</span><span style=\"font-size:19px;font-family:宋体\">项，校教改课题</span><span style=\"font-size:19px\">17</span><span style=\"font-size:19px;font-family: 宋体\">项。两名教师获得辽宁省思政课青年教师择优资助计划。获批教育部优秀教学团队项目两项，两名教师申报成功教育部“思政课重点难点问题解答”项目。完成教学研究专著多部。</span></p><p><strong><span style=\"font-size:19px;font-family:宋体\">四、思政课教师获得多项荣誉</span></strong></p><p><span style=\"font-size:19px\">&nbsp;&nbsp; </span><span style=\"font-size:19px;font-family:宋体\">一名教师被评为</span><span style=\"font-size:19px\">2014</span><span style=\"font-size:19px;font-family:宋体\">年度全国思政课教师年度影响力人物，一名教师被评为</span><span style=\"font-size:19px\">2015</span><span style=\"font-size:19px;font-family:宋体\">年度全国思政课教师影响力提名人物；三名教师入选辽宁省教学名师，两名教师入选辽宁省优秀教师。两名教师荣获沈阳市劳动模范。一名教师获得教育部全国思政课教学能手，四名教师获得辽宁省青年教师基本功大赛一等奖，获得辽宁省思政课教学能手称号。</span></p><p><strong><span style=\"font-size:19px\">&nbsp; &nbsp;&nbsp;</span></strong></p><p><br/></p>','管理员','1514809331','100','8') [ RunTime:0.0006s ]
